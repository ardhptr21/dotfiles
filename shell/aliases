# list directory
alias la="ls -lah"
alias ll="ls -lh"

# git
alias gp="git push"
alias gpf="git push -f"
alias gl="git log --oneline"
alias gc="git checkout"
alias ga="git add -A"
alias gs="git status"
alias gb="git branch"
alias gf="git fetch"
alias gpl="git pull"
alias gr="git reset"
alias grh="git reset --hard"
alias gst="git stash"
alias gstp="git stash pop"
alias gcm="git commit -m"
alias gcam="git commit --amend -m"
alias gcan="git commit --amend --no-edit"
